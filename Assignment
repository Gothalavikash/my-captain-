#include<stdio.h>
 void main()
{
    int n;
    printf("Enter student marks:");
    scanf("%d",&n);
    if (n>=85 && n<=100 )
    {
        printf("GRADE A");
    }
    else if(n>=70 && n<=84)
    {
        printf("GRADE B");
    }
    else if(n>=55 && n<=69)
    {
        printf("GRADE C");
    }
    else if(n>=40 && n<=54)
    {
        printf("GRADE D");
    }
    else
    {
        printf("GRADE F");
    }

}
